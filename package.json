{
  "name": "edb-node-chrome",
  "version": "0.4.2",
  "description": "Script de teste para o pacote npm chrome-launcher.",
  "main": "index.js",
  "config": {
    "commitMessage": "Atualização de segurança"
  },
  "scripts": {
    "dev": "node index.js",
    "git": "npm run gst && npm run gad && npm run gco && npm run gta && npm run gpo",
    "gst": "git status",
    "gad": "git add .",
    "gco": "cross-var git commit -m \"$npm_package_config_commitMessage\"",
    "gta": "cross-var git tag $npm_package_version",
    "gpo": "git push origin master --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digitalbocca/edb-node-chrome.git"
  },
  "keywords": [
    "chrome",
    "launcher",
    "test"
  ],
  "author": "Gabriel Bertola Bocca - gabriel at estudiodigitalbocca.com.br",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/digitalbocca/edb-node-chrome/issues"
  },
  "homepage": "https://github.com/digitalbocca/edb-node-chrome#readme",
  "dependencies": {
    "chrome-launcher": "^0.12.0"
  },
  "devDependencies": {
    "cross-var-no-babel": "^1.2.0",
    "standard": "^14.3.1"
  }
}
